<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.CustomersController">
   <children>
      <TableView fx:id="customersTable" layoutX="123.0" layoutY="82.0" prefHeight="568.0" prefWidth="642.0" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="74.0">
        <columns>
            <TableColumn fx:id="nameCol" minWidth="80.0" text="%name" />
          <TableColumn fx:id="address1Col" minWidth="80.0" text="%address1" />
            <TableColumn fx:id="address2Col" minWidth="80.0" text="%address2" />
            <TableColumn fx:id="postalCol" minWidth="80.0" text="%postalCode" />
            <TableColumn fx:id="phoneCol" minWidth="80.0" text="%phoneNumber" />
            <TableColumn fx:id="cityCol" minWidth="80.0" text="%city" />
            <TableColumn fx:id="countryCol" minWidth="80.0" text="%country" />
            <TableColumn fx:id="deleteCol" minWidth="20.0" prefWidth="32.0" resizable="false" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Button defaultButton="true" layoutX="562.0" layoutY="15.0" mnemonicParsing="false" onAction="#addCustomerBtn" prefHeight="34.0" prefWidth="80.0" text="%newCustomer" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="15.0">
         <font>
            <Font name="Segoe UI Semibold" size="14.0" />
         </font></Button>
      <Button layoutX="472.0" layoutY="15.0" mnemonicParsing="false" onAction="#updateCustomerBtn" prefHeight="34.0" prefWidth="80.0" text="%update" AnchorPane.rightAnchor="110.0" AnchorPane.topAnchor="15.0">
         <font>
            <Font name="Segoe UI" size="14.0" />
         </font></Button>
      <Button layoutX="382.0" layoutY="15.0" mnemonicParsing="false" onAction="#deleteCustomerBtn" prefHeight="34.0" prefWidth="80.0" text="%delete" AnchorPane.rightAnchor="200.0" AnchorPane.topAnchor="15.0">
         <font>
            <Font name="Segoe UI" size="14.0" />
         </font></Button>
   </children>
</AnchorPane>
